/*
https://www.codechef.com/SNCK1A19/problems/ARRGRAPH


*/

#include <bits/stdc++.h>
using namespace std;

const int N=60;
vector<int> g[N];
int arr[N],vis[N];
void dfs(int v)
{
    vis[v]=1;
    for(auto x:g[v])
        if(!vis[x])
        dfs(x);
}

int main()
{
    int prime[]={29, 31, 37, 41, 43, 47};
    int n,t0,tt;cin>>t0;
    while(t0--)
    {
        for(int i=0;i<N;i++){g[i].clear();vis[i]=0;}
        cin>>n;tt=0;
        for(int i=0;i<n;i++)cin>>arr[i];
        for(int i=1;i<n;i++)if(arr[i]!=arr[i-1])tt=1;
         
        for(int i=0;i<n-1;i++)
            for(int j=i+1;j<n;j++)
            if(__gcd(arr[i],arr[j])==1)
            {
                g[i].push_back(j);
                g[j].push_back(i);
            }
        dfs(0);    
        int t=1;
        for(int i=0;i<n;i++)if(vis[i]==0)t=0;
        if(t)
        {
            cout<<0<<endl;for(int i=0;i<n;i++)cout<<arr[i]<<" ";cout<<endl;
            continue;
        }
        if(tt)
        {
            cout<<1<<endl;int t1=1;
            for(int i=0;i<n;i++)if(arr[i]!=47 && t1==1){cout<<47<<" ";t1=0;} 
            else cout<<arr[i]<<" ";
            cout<<endl;
            continue;
        }

        int x=47;
        if(arr[0]==47)x=43;
        
        cout<<1<<endl;int t1=1;
        for(int i=0;i<n;i++)if(i==1)cout<<x<<" ";
        else cout<<arr[i]<<" ";
        cout<<endl;
        
        
    }
    return 0;
}
